<html>
  <head>
    <title>Merchant Check Out Page</title>
  </head>
  <body>
    <center><h1>Please do not refresh this page...</h1></center>

    <form
      method="post"
			{/*action="https://pguat.paytm.com/oltp-web/processTransaction"*/}
			action=<%= 'https://securegw-stage.paytm.in/theia/processTransaction?ORDER_ID=' + restdata.ORDER_ID %>
      name="f1"
    >
      <table border="1">
        <tbody>
          <% for (name in restdata) {
            %>
          <input
            type="hidden"
            name="<%= name %>"
            value="<%= restdata[name] %>"
          />

          <%}%>
        </tbody>
      </table>
      <script type="text/javascript">
        document.f1.submit();
      </script>
    </form>
  </body>
</html>
